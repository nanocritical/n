import nlang.meta
import nlang.literal
from nlang.numbers import *
from nlang.prelude import *

fun malloc len:size = @!u8
fun realloc p:?@!u8 old_len:size len:size = @!u8
fun free p:?@!u8 len:size = void

fun memcpy dst:@!u8 src:@u8 el_size:size n:size = void

(fun d s) cast src:s = d

(fun t r:(nlang.meta.nullable_ref t)) nonnull_cast x:r = r.nonnull
  return (cast r.nonnull r) x
