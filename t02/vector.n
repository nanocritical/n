from n import *

alias v32 = Buf I32

fun foo = v32
	let w = {1 0 2}:(Slice I32)
	assert w.[0]* == 1
	assert w.[1]* == 0
	assert w.[2]* == 2
	let r = {}:v32 such
		r#Append w
	return r

fun Main = I32
	let v = {}:v32
	such
		(v#Push)! = 0
		(v#Push)! = 1
		assert v.Last == 1
		v#Pop
		(v#Push)! = 1
		for i in v.All.Iter
			assert v.[i]* == i.Trim_u32.Signed
	let w = foo
	assert w.[0]* == 1
	assert w.[1]* == 0
	assert w.[2]* == 2
	return v.[1]* - 1
