import range

intf Container T:Any =
  method size = Size

intf MutableContainer T:Any = (Container T)
  method! clear = Void

intf Map Idx:Any T:Any =
  method get__ n:Idx = T
    ! range.IndexRange.validindex this n
  method! set__ n:Idx el:T = Void
    ! range.IndexRange.validindex this n

intf OrderedMap Idx:Ordered T:Any = (Map Idx T)

intf Array T:Any = (OrderedMap Size T)
