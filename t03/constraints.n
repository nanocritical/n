from n import *

union u =
	| A = 0
	| B -> I32
	| C -> U32

fun aux e:u = I32
	match e
	| B
		return e.B
	| _
		return 0

fun set1 e:!u
	match e*
	| B
		e!B = 1
	| _
		noop

fun Main = I32
	let g = {}:u.A
	match g
	| B -> return 1
	| C -> return 1
	| _ -> noop

	let f = {}:u.B
	Prn f.B
	set1 !f
	Prn f.B

	let e = 1:u.B
	match e
	| B
		assert e.B != 0

	return (aux e) - 1
