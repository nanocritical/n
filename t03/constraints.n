from nlang import *

union u =
  | A
  | B
    x	:i32
  | C
    x	:u32

export fun aux e:u = i32
  match e
  | B
    return e.B.x
  | _
    return 1

export fun main = i32
  let g = {}:u.A
  match g
  | B -> return 1
  | C -> return 1
  | _ -> noop

  let f = {}:u.B
  say f.B.x

  let e = {x=1}:u.B
  match e
  | B
    assert <- e.B.x == 0

  return aux e
