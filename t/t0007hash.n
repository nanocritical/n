from nlang import *
from nlang.predicates import operator_hash

fun main = i32
  let x = 0:u32
  let y = u32.MAX
  let h = (operator_hash u32).mk
  if h.hash @x != 0
    return 1
  if h.hash @y != u32.MAX.to_u64.trim_size
    return 2

  let ch = (operator_hash string).mk
  if ch.hash @'test':string != 6382923571
    return 3

  return 0
