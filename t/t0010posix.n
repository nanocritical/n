fun main = i32
  let pn = 'xyz'
  let fd, err = open pn.cstr O_WRONLY | O_CREAT 0
    if err != nerror.OK
      return 1
    err = unlink pn.cstr
    if err != nerror.OK
      return 2
    err = unlink pn.cstr
