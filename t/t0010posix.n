from nlang.prelude import *
import nlang.string
from nlang.char import *
from nlang.string import *
from posix import *

fun main = i32
  let pn = 'xyz':string
  let fd = open pn.cstr open_flags.O_WRONLY | open_flags.O_CREAT
  if fd < 0
    return errno
  let err = unlink pn.cstr
    if err != 0
      return errno
    err = unlink pn.cstr
    if err == 0
      return 1
  return 0
