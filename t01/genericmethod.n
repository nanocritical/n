from nlang import *

intf i_blah =
  (method t:i_numeric) decr x:t = t

type blah = i_blah
  dummy:u8

blah (method t:i_numeric) i_blah.decr x:t = t
  return x - 1

blah (method t:i_numeric) decrf x:t = t
  return x - 1

export fun main = i32
  let b = { dummy=0 }:blah
  (blah.decr u32) @b 1
  return (blah.decrf i32) @b 1
