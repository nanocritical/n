from nlang import *

intf `blah =
  (met t:`integer) decr x:t = t

struct blah =
  isa `blah
  dummy:u8

blah (met t:`integer) `blah.decr x:t = t
  return x - 1

blah (met t:`integer) decrf x:t = t
  return x - 1

export fun main = i32
  let b = { dummy=0 }:blah
  let _ = (blah.decr u32) b 1
  return (blah.decrf i32) b 1
