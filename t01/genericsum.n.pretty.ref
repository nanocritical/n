from nlang import *
type container = i_trivial_ctor
  let this = container
  let final = container
  x:t

container method add self:@!final = void
  nlang.builtins.i_numeric.operator_assign_plus @self.x @x

export fun main = i32
  let v = container i32{{ x=0 }}
    t01.genericsum.container.add @!v 1
  return v.x - 1
