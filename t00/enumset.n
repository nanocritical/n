from n import *

enum en =
	| A
	| B
	| C

enum en0 =
	| A = 0
	| B
	| C

fun tests
	let m = {A B C}:(Enumset en)
	assert m.Count == 3
	assert A in m
	assert B in m
	assert C in m
	assert {A B C} == m
	assert ~{} == m

	assert {B C} < m
	assert {B C} <= m
	assert m > {A C}
	assert m >= {A C}
	assert ~{A}:(Enumset en) == {B C}

	assert {A}:(Enumset en) & {B} == {}:(Enumset en)
	assert ({A}:(Enumset en) | {B}).Count == 2

	Prn m
	Prn {B C}:(Enumset en)

fun Main = I32
	tests
