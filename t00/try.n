from n import *

fun inferred = Error
	try
		throw INVAL
	catch err
		return err

fun Foo = I32
	try
		throw ONE INVAL
		throw ZERO Error.OK
		return 2
	catch ZERO _
		return 0
	catch ONE _
		return 1

fun bar = I32
	try
		let except = Error.INVAL
		return 0
	catch err
		try
			Drop err
			let except ONE = Error.INVAL
			let except ZERO = Error.OK
			return 0
		catch ZERO _
			return 0
		catch ONE err
			Drop err
			return 1

fun noerror = I32
	try
		let except = Error.OK
		return 0
	catch _
		return 1

fun Main = I32
	assert inferred != Error.OK
	assert Foo == 1
	assert bar == 1
	assert noerror == 0
	try
		throw Error.INVAL
		return 1
	catch err
		Drop err
		return 0
