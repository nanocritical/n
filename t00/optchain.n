from n import *

struct a =
	x	:I32
	y	:?@!a

a met! clear
	self!x = 0

a met$ getx = @$I32
	return @$self$x

fun Main = I32
	let xx = {x=1}:a
	-- FIXME: operator precedence, parentheses shouldn't be needed.
	Assert <- (xx.y?.x)? == (xx.y)?

	Assert <- (nullable xx.getx)?
	Assert <- not (xx.y?.getx)?

	xx!clear
	Assert <- not (xx.y?!clear)?

	let v = xx.y?.x
	if v? and v == 1
		return 1

	xx!y = @!{x=1}:a
	let w = xx.y?.x
	if w? and w == 1
		return 0

	return 1
