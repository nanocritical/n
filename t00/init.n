from nlang import *

type tt = i_trivial_copy
  x:i32
  y:i32

type t = i_trivial_copy
  x:i32
  y:tt

fun foo x:i32 y:i32 = i32
  return x

export fun main = i32
  let a = t{
    x = 1
    y = tt{
      x = 1
      y = 0
    }
  }
  let b = (foo
             (foo 0
                1)
             1)
  return b + a.y.x - 1
