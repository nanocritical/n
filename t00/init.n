from nlang import *

struct tt = i_trivial_copy
  x:i32
  y:i32

struct t = i_trivial_copy
  x:i32
  y:tt

fun foo x:i32 y:i32 = i32
  return x

export fun main = i32
  let a = {
    x = 1
    y = {
      x = 1
      y = 0
    }
  }:t
  let b = (foo
             (foo 0
                1)
             1)
