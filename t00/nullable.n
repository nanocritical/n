from n import *

struct a =
	x	:I32

a met getx = I32
	return self.x

fun foo v:?*a = I32
	if v?
		return v.getx
	else
		return 0

fun mk_nullable = ?*I32
	let r = Alloc I32
	r! = 1
	return r

fun Main = Error
	assert 0 == foo {}
	let p = nullable *1:I32
	if not p?
		return INVAL
	if not mk_nullable?
		return OK
	let q = mk_nullable
	let x = q else 0
	if 0 != x - 1
		return INVAL
