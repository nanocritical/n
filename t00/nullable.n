from n import *

struct a =
	x	:I32

a met getx = I32
	return self.x

fun foo v:?@a = I32
	if v?
		return v.getx
	else
		return 0

fun mk_nullable = ?@I32
	let r = Alloc I32
	r! = 1
	return r

fun Main = I32
	assert 0 == foo {}
	let p = nullable @1:I32
	if not p?
		return 1
	if not mk_nullable?
		return 0
	let q = mk_nullable
	let x = q else 0
	return x - 1
