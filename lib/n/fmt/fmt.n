from n.builtins import *
import n.io

intf `State =
	isa io.`Writer
	met Width = Bool, Uint
	met Precision = Bool, Uint
	met Flag c:Rune = Bool

inline struct State =
	isa `State
	w	:@#io.`Writer

State met# Init w:@#io.`Writer
	self#w = w

State met Width = Bool, Uint
	return false, 0

State met Precision = Bool, Uint
	return false, 0

State met Flag c:Rune = Bool
	return false

State met# Write b:[]U8 = []U8, Error
	return self#w#Write b
