from nlang.literals export *
from nlang.builtins export *
from nlang.containers export *
from nlang.memory export *
from nlang.chars export *

from nlang.strings import *

extern fun write_buf s:@u8 count:size  = void

export fun say a:@i_show = void
  let s = string.mk
    a.show @#s
  let b = s.bytes_utf8
  write_buf b.unsafe_data b.bytecount
  let eol = '\n':char
  write_buf @eol.code.trim_u8 1
